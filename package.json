{
  "name": "jfwk-to-afwk",
  "displayName": "Jfwk-to-Afwk",
  "description": "Transforma campos Java con JPA en propiedades TypeScript con decoradores para fwk Setting.",
  "version": "0.0.4",
  "publisher": "Moncho",
  "engines": {
    "vscode": "^1.101.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MonchoGrande/jfwk-to-afwk.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:Jfwk-to-Afwk.convert"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Jfwk-to-Afwk.convert",
        "title": "Java to Angular"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "Jfwk-to-Afwk.convert",
          "when": "resourceLangId == java",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "Jfwk-to-Afwk.convert",
          "when": "editorLangId == java",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}